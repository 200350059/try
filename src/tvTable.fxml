      <TableView fx:id="tableView" layoutX="200.0" layoutY="125.0" prefHeight="200.0" prefWidth="295.0">
        <columns>
          <TableColumn fx:id="brandColumn" prefWidth="75.0" text="Brand" />
          <TableColumn fx:id="sizeColumn" prefWidth="75.0" text="Size" />
            <TableColumn fx:id="priceColumn" prefWidth="75.0" text="Price" />
            <TableColumn fx:id="imageColumn" prefWidth="75.0" text="Image" />
        </columns>
      </TableView>
      <TextField fx:id="searchBar" layoutX="51.0" layoutY="351.0" />
      <Button layoutX="200.0" layoutY="351.0" mnemonicParsing="false" onAction="#search" text="Search" />
   </children>
</AnchorPane>
